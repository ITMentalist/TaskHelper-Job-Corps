' LOCAL PROCEDURES
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Private Sub Class_Initialize()
    pErrorFlag = False
    pErrorMessage = "The Student File has encountered the following issues:" & vbNewLine & vbNewLine
    pModCheckFlag = False
    pModCheckMessage = "The following class mods are less than 8 characters; please confirm before proceeding:" & vbNewLine & vbNewLine
    MaxCharactersName = 38
    StandardLength = 8
    pAddressBookListingExists = False
    pFullProgramFlag = False
End Sub

Private Function RemoveSym(ByVal passTextbox As String) As String
    'Removes the currency symbol from the textbox output. The textboxes have event handlers _
        that prevent entering non-numbers; however, copy/paste can bypass this restriction.
    Dim str As String
    str = passTextbox
    RemoveSym = Trim(Replace(str, "$", " "))
End Function

Private Sub Class_Terminate()
    'Empty
End Sub

