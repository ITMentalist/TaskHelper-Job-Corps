''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetMailingName property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetMailingName() As String
    If Len(Trim(GetMiddleName)) > 0 Then
    	GetMailingName = GetFirstName & Space(1) & GetMiddleName & Space(1) & GetLastname
    Else
    	GetMailingName = GetFirstName & Space(1) & GetLastname
    End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetAlphaName property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetAlphaName() As String
	Dim str As String
	If Len(Trim(GetMiddleName)) > 0 Then
		str = GetLastname & Space(1) & GetFirstName & Space(1) & GetMiddleName
	Else
		str = GetLastname & Space(1) & GetFirstName
	End If
	If Len(str) < MaxCharactersName Then
		GetAlphaName = str
	Else
		pErrorFlag = True
		pErrorMessage = pErrorMessage & "* The student's combined Alpha Name must be less than 38 characters" & vbNewLine & vbTab & vbTab & str & vbNewLine
	End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetSalesperson property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetSalesperson() As String
	If IsNumeric(SalespersonTextbox) And Len(Trim(SalespersonTextbox)) = 4 Then
		GetSalesperson = SalespersonTextbox
	Else
		pErrorFlag = True
		pErrorMessage = pErrorMessage & "* The salesperson code must be 4 numbers" & vbNewLine
	End If
End Function

