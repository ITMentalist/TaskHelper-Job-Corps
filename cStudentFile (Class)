	    GetProgramNumber = vbNullString
		pErrorFlag = True
		pErrorMessage = pErrorMessage & "* The Program Number must be numeric and 8 digits long" & vbNewLine
	End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetProgramTuition property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetProgramTuition() As String
	If IsNumeric(RemoveSym(ProgramTuitionTextbox)) And Val(RemoveSym(ProgramTuitionTextbox)) >= 0 Then
		GetProgramTuition = RemoveSym(ProgramTuitionTextbox)
	Else
	    GetProgramTuition = vbNullString
		pErrorFlag = True
		pErrorMessage = pErrorMessage & "* The Program Tuition must be a non-negative number"
	End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetClassMod1 property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetClassMod1() As String
	GetClassMod1 = Trim(ClassMod1Textbox)
	If Len(GetClassMod1) >= 1 And Len(ClassMod1Textbox) < StandardLength Then
	    GetClassMod1 = vbNullString
		pModCheckFlag = True
		pModCheckMessage = pModCheckMessage & vbTab & Trim(ClassMod1Textbox) & vbNewLine
	End If
End Function

