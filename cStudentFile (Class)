    Else
        GetStudentTuition = vbNullString
        pErrorFlag = True
        pErrorMessage = pErrorMessage & "* Student Tuition must be a non-negative number" & vbNewLine
    End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
