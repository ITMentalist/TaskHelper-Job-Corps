''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetNumberOfMonths property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetNumberOfMonths() As String
	If IsNumeric(RemoveSym(NumberOfMonthsTextbox)) And Val(RemoveSym(NumberOfMonthsTextbox)) >= 0 Then
		GetNumberOfMonths = RemoveSym(NumberOfMonthsTextbox)
	Else
	    GetNumberOfMonths = vbNullString
		pErrorFlag = True
		pErrorMessage = pErrorMessage & "* The number of months must be a non-negative integer"
	End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' CalcDollarPerMonth property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function CalcDollarPerMonth() As String
    Dim Total As Currency
    Dim DownPay As Currency
    Dim Installment As Integer
    Dim Temporary As Currency
    If GetPaymentType = "PayByNumber" Then
        Total = CCur(GetStudentInvoice)
        DownPay = CCur(GetDownPay)
        Installment = CInt(GetNumberOfMonths)
        Temporary = Total - DownPay
        If Installment < 1 Then
            CalcDollarPerMonth = vbNullString
            pErrorFlag = True
		    pErrorMessage = pErrorMessage & "* Calculation Error: The number of installments cannot be less than 1"
		ElseIf Temporary < 1 Then
            CalcDollarPerMonth = vbNullString
            pErrorFlag = True
		    pErrorMessage = pErrorMessage & "* Calculation Error: The Student Invoice minus the Down Payment cannot be less than 1"
		Else
		    'Note: Backslash indicates integer division
		    'Technical Debt Note: Integer division seems to be the closest approximation to the AS/400's monthly payment _
		        calculation, barring the creation of a more extensive custom division operator.
		    CalcDollarPerMonth = CStr(Temporary \ Installment)
		    pFinalPayment = CStr(Temporary - Installment * Monthly)
		End If
    Else
        CalcDollarPerMonth = vbNullString
        pErrorFlag = True
		pErrorMessage = pErrorMessage & "SCRIPT ERROR: The monthly payments are being calculated without the correct user entry"
    End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetDollarPerMonth property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetDollarPerMonth() As String
	If IsNumeric(RemoveSym(DollarPerMonthTextbox)) And Val(RemoveSym(DollarPerMonthTextbox)) >= 0 Then
		GetDollarPerMonth = RemoveSym(DollarPerMonthTextbox)
	Else
	    GetDollarPerMonth = vbNullString
		pErrorFlag = True
		pErrorMessage = pErrorMessage & "* The monthly payment amount must be a non-negative number"
	End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' CalcNumberOfMonths property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function CalcNumberOfMonths() As String
    Dim Total As Currency
    Dim DownPay As Currency
    Dim Monthly As Currency
    Dim Temporary As Currency
    If GetPaymentType = "PayByDollars" Then
        Total = CCur(GetStudentInvoice)
        DownPay = CCur(GetDownPay)
        Monthly = CCur(GetDollarPerMonth)
        Temporary = Total - DownPay
        If Monthly < 1 Then
            CalcNumberOfMonths = vbNullString
            pErrorFlag = True
		    pErrorMessage = pErrorMessage & "* Calculation Error: The monthly payment amount cannot be less than 1"
        ElseIf Temporary < 1 Then
            CalcNumberOfMonths = vbNullString
            pErrorFlag = True
		    pErrorMessage = pErrorMessage & "* Calculation Error: The Student Invoice minus the Down Payment cannot be less than 1"
		Else
		    'Note: Backslash indicates integer division
		    CalcNumberOfMonths = CStr(Temporary \ Monthly)
		    pFinalPayment = CStr(Temporary - CInt(CalcNumberOfMonths) * Monthly)
		End If
    Else
        CalcNumberOfMonths = vbNullString
        pErrorFlag = True
		pErrorMessage = pErrorMessage & "SCRIPT ERROR: The number of months is being calculated without the correct user entry"
    End If
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetFinalPayment property
'Set by the payment calculations
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Property Get GetFinalPayment() As Boolean
    If Val(pFinalPayment) >= 1 Then
        GetFinalPayment = pFinalPayment
    Else
        GetFinalPayment = "0"
    End If
End Property

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetSession property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetSession() As String
	GetSession = SessionComboBox
End Function

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' GetParentNumber property
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Public Function GetParentNumber() As String
    Dim str
    str = Left(Trim(ParentNumberComboBox.Value), 8)
    If IsNumeric(str) Then
        GetParentNumber = str
    Else
        If GetParentStatus = True Then
            pErrorFlag = True
		    pErrorMessage = pErrorMessage & "* '" & str & "' is not recognized as a parent number"
        End If
    End If
End Function

